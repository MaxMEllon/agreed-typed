{
  "name": "agreed-typed",
  "version": "0.0.1",
  "main": "./lib/types.js",
  "types": "./lib/types.d.ts",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "tslint ./src/**/*.ts",
    "fmt": "tslint --fix",
    "gen-swagger": "ts-node src/generator.ts",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@types/power-assert": "^1.5.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "get-port": "^4.0.0",
    "jest": "^23.6.0",
    "power-assert": "^1.6.1",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "helper.ts"
    ]
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^10.12.0",
    "agreed-core": "git+https://github.com/recruit-tech/agreed-core#master",
    "axios": "^0.18.0",
    "minimist": "^1.2.0",
    "typescript-json-schema": "^0.32.0"
  }
}
